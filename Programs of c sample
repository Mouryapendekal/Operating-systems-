#include <stdio.h>
void processmethod(int a[], int n);
int main()
{
  int a[]={7,9,3,1,99};
int N= sizeof(a)/ sizeof(int);
processmethod(a,N);
int i;
for(i=0;i<N;i++)
  printf("%d ", a[i]);
return 0;
}
void processmethod(int a[],int N)
{
  int i;
  for(i=0;i<N;i++)
  {
    if (a[i]%2!=0)
    a[i]=a[i]+1;
    }
    }
